<templateSet group="Kotlin">
  <template name="testm" value="@Test&#10;fun test$A$() {&#10;    $END$&#10;}" description="add test method" toReformat="true" toShortenFQNames="true">
    <variable name="A" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="KOTLIN" value="true" />
    </context>
  </template>
  <template name="testc" value="@Test&#10;fun test$A$() = runBlocking {&#10;    $END$&#10;}" description="add test method coroutine" toReformat="true" toShortenFQNames="true">
    <variable name="A" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="KOTLIN" value="true" />
    </context>
  </template>
  <template name="tname" value="Thread.currentThread().name" description="current thread name" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="KOTLIN" value="true" />
    </context>
  </template>
</templateSet>